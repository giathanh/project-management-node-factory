{
  "name": "project-management-node-factory",
  "description": "factory for nodejs project management",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "http-status": "^1.4.2",
    "setprototypeof": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "eslint": "^7.13.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build": "tsc",
    "check": "npm run tslint && npm run prettier && npm audit",
    "clean": "rimraf ./lib \"npm-debug.log*\" ./coverage ./.nyc_output",
    "doc": "jsdoc -c jsdoc.json",
    "test": "npm run check",
    "tslint": "tslint --project tsconfig.json -c tslint.json --exclude \"**/*.spec.ts\" \"src/**/*.ts\"",
    "preversion": "npm run clean && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prepublishOnly": "npm run clean && npm run build && npm test",
    "prettier": "prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giathanh/project-management-node-factory.git"
  },
  "author": "thanh.clg",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/giathanh/project-management-node-factory/issues"
  },
  "homepage": "https://github.com/giathanh/project-management-node-factory#readme"
}
